MACHINE
    TiposComuns

CONCRETE_CONSTANTS
    MAX_INT

PROPERTIES
    MAX_INT : NAT1 &
    MAX_INT = 1000000

SETS
    VAGAS ; // Conjunto das vagas
    TIPOS = { idoso , deficiente , comum } ; // Tipo da vaga
    STATUS_VAGA = { livre , ocupada } ; // status da vaga
    CORES = { azul , amarela , verde , vermelha } ; // Cor da lâmpada que fica na vaga
    SIM_NAO = { sim , nao }

END
